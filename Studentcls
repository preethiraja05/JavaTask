package dailytask;

class Student {
    // Properties
    String name;
    int age;

    // Constructor
    Student(String a, int b) {
        name = a;
        age = b;
    }

   
    void display() {
        System.out.println("Name: " + name);
        System.out.println("Age: " + age);
    }
}

public class Studentcls {
    public static void main(String[] args) {
        // Creating mul objects 
        Student s1 = new Student("Preethi", 20);
        Student s2 = new Student("Aakash", 20);
        Student s3 = new Student("Arthy", 20);
        Student s4 = new Student("Ashwin", 20);

        // for Displaying the  values
        s1.display();
        s2.display();
        s3.display();
        s4.display();
    }
}
