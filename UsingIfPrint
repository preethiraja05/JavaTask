package dailytask;
public class PrintusingIf {
	public static void main(String agrs[]) {
		int a=10;
		if(a==10) {
			a--;
			System.out.println(a);
			a--;
			System.out.println(a);
			a--;
			System.out.println(a);
			a--;
			System.out.println(a);
			a--;
			System.out.println(a);
			a--;
			System.out.println(a);
			a--;
			System.out.println(a);
			a--;
			System.out.println(a);
			a--;
			System.out.println(a);
		}
		
	}

}
